<event id="1800" name="贝壳">
	<quest id="254" new="1">
		<embranchment id="1">
			<conditions>
				<var type="Equal" name="state" value="1"/>
				<var type="Less" name="get_贝壳" value="10"/>
			</conditions>
			<actions>
				<notify content="你得到了一个贝壳"/>
				<var type="Add" name="get_贝壳" value="1"/>
				<refresh name="get_贝壳"/>
			</actions>
		</embranchment>
		<embranchment id="2">
			<conditions>
				<var type="Equal" name="state" value="1" id="254"/>
				<var type="Great" name="get_贝壳" value="9"/>
			</conditions>
			<actions>
				<var type="Set" name="state" value="-3" id="254"/>
				<refresh name="state"/>
				<refresh name="get_贝壳"/>
				<notify content="恭喜你完成了收集贝壳的任务,回大厨沙红芋(330,299)处交任务吧"/>
				<notify1 content="大厨沙红芋(330,299)"/>
			</actions>
		</embranchment>
	</quest>
</event>
