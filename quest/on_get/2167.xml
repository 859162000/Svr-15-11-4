<event id="2167" name="石兵零件">
	<!--new means new quest, default is quest on user-->
	<quest id="390" new="1">
		<embranchment id="1">
			<conditions>
				<var type="Equal" name="state" value="1" id="390"/>
				<var type="Less" name="get_石兵零件" value="10"/>
			</conditions>
			<actions>
				<notify content="你得到了一个石兵零件"/>
				<var type="Add" name="get_石兵零件" value="1"/>
				<refresh name="get_石兵零件"/>
			</actions>
		</embranchment>
		<embranchment id="2">
			<conditions>
				<var type="Equal" name="state" value="1" id="390"/>
				<var type="Great" name="get_石兵零件" value="9"/>
			</conditions>
			<actions>
				<var type="Set" name="state" value="-3" id="390"/>
				<refresh name="state"/>
				<refresh name="get_石兵零件"/>
				<notify content="你已经收集到了10个石兵零件,回皇城的鲁建瓴(810,640)处交任务吧"/>
				<notify1 content="皇城-鲁建瓴(810,640)"/>
			</actions>
		</embranchment>
	</quest>
</event>
