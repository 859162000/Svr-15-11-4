	<quest id="20558" new="1">
		<embranchment id="1">
			<conditions>
				<var type="Equal" name="state" value="1" id="20558"/>
				<var type="Less" name="get_百年老酒" value="10"/>
			</conditions>
			<actions>
				<notify content="你得到了一坛百年老酒"/>
