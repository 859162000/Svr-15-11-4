	<npc name="天机老人" id="146">
		<menu>function MainDialog()                                                                       
this:AddMakeItem("特殊道具包裹加工(0)","id[811,662,663,664]","包裹")
this:AddMakeItem("特殊道具马铠加工(0)","id[880,878,883-887]","马铠")
this:AddMakeItem("特殊道具卷轴加工(0)","id[668,670,671,672,673,674,675,676]","卷轴")
this:AddMakeItem("银钥匙(0)","id[798]","道具")
TaskDialog()                                                                               this:AddSeptAttackNpc("家族争夺NPC(0)")
this:AddSeptNpcCustom("管理费(0)") 	                                                    this:AddWarOperator("控制NPC家族查询(0)","4")                                                this:AddWarOperator("下场NPC争夺战查询(0)","5")
this:SeptGiveupOccupy("族长放弃当前占领(0)")
this:AddDialogItem("关于家族争夺NPC和管理费(3)","Dialog1") 
end
this:PlaySound("22018.wav,21000.wav,21001.wav,21002.wav,21003.wav,21004.wav,210005.wav,210006.wav,21007.wav,210008.wav,21009.wav,60021.wav") 
function Dialog1()                                           
this:AddTalk("关于家族争斗NPC和管理费: 家族争夺战需要族长支付给可占领NPC40两进行申请，如果该NPC没有被其他家族占领，那么该NPC属于申请家族，占有的家族可以在该NPC处每天领取管理费。\n如果该NPC已经有占领家族了，那么想抢夺NPC的家族可以在每天18：00点以前发起对占领家族的挑战，挑战开始时间为当天的19：00点，19：10结束。在19：00点前双方家族成员必须到抢夺的目标NPC周围，19：00点准时开始时只有在NPC身边的家族成员才能够参加争夺战。争夺战时请不要离开NPC太远，不然剥夺战斗资格。\n当一方家族全部被消灭或者到结束时间，有战斗权限成员多的一方家族获胜，获胜家族获得NPC控制权，如果双方平手，那么防守方获胜。")
this:AddDialogItem("离开","MainDialog")
end  
this:AddDialog("MainDialog")                                                                           if( IsHasTask() == false) then
this:AddDialog("MainDialog")
else
this:AddDialog("MainDialog")
end</menu>
