		<menu>function MainDialog()                                                                        TaskDialog()
this:AddTong("建立帮会(0)")
this:AddDialogItem("攻城战相关(0)","Dialog6")                     
this:AddWarOperator("城市控制帮会查询(0)","2")
this:AddDialogItem("国王权利相关(0)","Dialog7")
this:AddDialogItem("国家军队相关(0)","Dialog5")
this:AddDialogItem("皇帝的权限(0)","Dialog8")
this:AddDialogItem("关于任命功能(0)","Dialog9")
this:AddDialogItem("关于帮会(3)","Dialog2")
this:AddDialogItem("关于帮会行动力(3)","Dialog4") 
this:AddDialogItem("关于帮会争夺和管理费(3)","Dialog3")  
end
this:PlaySound("23020.wav,23011.wav,23015.wav,23022.wav,23020.wav,20001.wav,20003.wav,20005.wav,61006.wav,61005.wav")                            

function Dialog1()
this:AddTalk("关于烽火台：国战期间，国王有权点燃自己国家的烽火台，一旦点燃烽火台，国王可以选择不同等级的国民传送到边境的边防官处，以便参加国战。")   
this:ArmymenLevel("召唤等级30以上国民(0)",30) 
this:ArmymenLevel("召唤等级40以上国民(0)",40)  
this:ArmymenLevel("召唤等级50以上国民(0)",50)                                                   
this:AddDialogItem("离开","MainDialog")
end
 
function Dialog2()
this:AddTalk("关于帮会:希望成为帮主的玩家必须到达45级，然后完成帮会任务，拿天羽令并准备5锭金找大臣，,提出成立帮会的申请，帮主填写帮会名称和帮会介绍。大臣处会列出所有正在申请的帮会名称和帮主名称。\n帮会成立以后，帮主不能离开帮会，除非将帮主让位给其他家族族长，帮会只剩一个家族的时候，帮会离开帮会才能解散帮会。\n当帮会成立以后，招人需要遵循以下规则：所招的玩家必须是本国家族族长，族长加入帮会后其族人自动成为该帮会成员，每个帮会暂时只能招20个家族。\n帮会成员离开帮会的途径：族长如果离开，整个家族都自动脱离帮会；如果家族成员想离开帮会必须在家族界面内选择离开家族，帮主只能够开除族长，如果是族人必须由族长开除出家族才会脱离帮会。")
this:AddDialogItem("离开","MainDialog")
end  
                                                                                                                
function Dialog3()
this:AddTalk("关于帮会争夺和管理费: 想申请攻城战的帮会可以由帮主去找该城市的大臣交钱发起攻城战，攻城战每天19：30开始，20：30结束。攻击方如果破坏该城市的城旗，那么夺城成功，如果在结束时间前没有破坏城旗，那么防守方获胜。 获胜的一方可以由帮主每天向大臣领取管理费。")
this:AddDialogItem("离开","MainDialog")
end                             
function Dialog4()
this:AddTalk("行动力是整个帮会展开活动的根本，每天系统会根据家族威望来给每个帮会一定的行动力值，每100点威望会有1点行动力。\n帮会的威望是由帮会中家族声望相加产生的。\n想要获得帮会行动力，提升帮会威望是唯一途径。")
this:AddDialogItem("离开","MainDialog")
end

function Dialog5()
this:AddTalk("行动力是整个帮会展开活动的根本，每天系统会根据家族威望来给每个帮会一定的行动力值，每100点威望会有1点行动力。\n帮会的威望是由帮会中家族声望相加产生的。\n想要获得帮会行动力，提升帮会威望是唯一途径。")
this:CreateArmy("建立军队(0)")
this:QueryArmy("查询军队(0)")	
this:LeaveArmy("离开军队(0)")
this:RecruitArmyer("招收队员(0)")
this:AddDialogItem("离开","MainDialog")
end

function Dialog6()
this:AddDialogItem("申请攻城战-2锭(0)","Dialog11")  
this:AddTongTownCustom("管理费(0)")                                                      
this:UnionGiveupOccupy("帮主放弃当前占领(0)")   
this:AddWarOperator("攻城战斗查询(0)","3")
this:AddSeptTownExp("领取经验(0)")
this:AddDialogItem("离开","MainDialog")
end

function Dialog7()
this:AddChoiceWarField("发起国战(0)")
this:AddDialogItem("点燃烽火台(0)","Dialog1")  
this:KingStopOneTalk("国王禁言(0)")
this:AddImmureDlg("囚禁：有权每天囚禁本国国民1小时(0)")
this:AddDialogItem("离开","MainDialog")
end

function AddInputBox1()
this:AddInputDialog("请输入你要禁言的玩家名字:",1)
this:AddDialogItem("离开","MainDialog")
end


function Dialog8()
this:AddDialogItem("禁言(0)--每天只能禁言10个玩家","AddInputBox1")
this:AddDialogItem("离开","MainDialog")
end

function Dialog9()
this:AddAppointMember("国王任命捕头(0)",3)
this:AddAppointMember("国王撤消捕头(0)",4)
this:AddDialogItem("离开","MainDialog")
end

function Dialog11()    
this:AddTongAttackTown("攻打宋国凤凰城(0)","2")
this:AddTongAttackTown("攻打魏国凤凰城(0)","3")
this:AddTongAttackTown("攻打齐国凤凰城(0)","4")
this:AddTongAttackTown("攻打燕国凤凰城(0)","5")
this:AddTongAttackTown("攻打唐国凤凰城(0)","7")
this:AddTongAttackTown("攻打汉国凤凰城(0)","8")
this:AddTongAttackTown("攻打吴国凤凰城(0)","9")
this:AddTongAttackTown("攻打赵国凤凰城(0)","10")
this:AddTongAttackTown("攻打周国凤凰城(0)","11")
this:AddTongAttackTown("攻打楚国凤凰城(0)","12")
this:AddDialogItem("离开","Dialog6")
end

this:AddDialog("MainDialog")
</menu>
	</npc>
