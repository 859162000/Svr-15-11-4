this:AddMakeItem("打造防具(0)","id[10006-10015,10056-10065,10106-10115,10156-10165,10206-10215,10256-10265,10761-10780,10811-10832,10909-10930,10861-10880]","打造防具")                                                              this:AddMakeItem("打造卷轴(0)","id[668,670,673,674,675,676]","打造卷轴")                                      this:AddBackoutItem("拆除装备(0)","type[101,102,103,104,105,106,107,108,109,110,111,112,113,114,115,116,117,118]","拆除装备")             this:AddHoleMakeItem("增加凹槽(0)","type[101,102,103,104,105,106,107,108,109,110,111,113,114,115,116,117,118]","增加凹槽") this:AddDialogItem("关于打造(3)","Dialog1")                                        
