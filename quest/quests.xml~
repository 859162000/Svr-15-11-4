end

function BuyCartoonPet()
this:AddTalk("欢迎啊。我这里有很多可爱美丽聪明的宠物。有可爱的奇异蛋，美丽的小仙子，憨厚的懒懒熊，敏捷的功夫兔，那么你看中哪个了呢？")
this:AddBuyCartoonPetCmd("购买奇异蛋需要1两(1)",9001)
this:AddBuyCartoonPetCmd("购买小仙子需要1两(1)",9002)
this:AddBuyCartoonPetCmd("购买懒懒熊需要1两(1)",9003)
this:AddBuyCartoonPetCmd("购买功夫兔需要1两(1)",9004)
this:AddDialogItem("返回","MainDialog")
end
function ConfirmSaleCartoon(cartoon)
this:AddTalk("确认卖出你的宠物吗?只能卖三文。")
this:ConfirmSaleCartoonPet("确定",cartoon)
this:AddDialogItem("返回","SaleCartoonPet")
end
function SaleCartoonPet()
num = this:GetCartoonPetNum()
if( num &gt; 0 ) then
for i=1,num do  
this:AddSaleCartoonPetCmd(i,"ConfirmSaleCartoon")
end
end
this:AddDialogItem("返回","MainDialog")
end



this:AddDialog("MainDialog")
