	<npc name="劳务官" id="5028">
		<menu>function MainDialog()
if( IsHasTask() == false) then
this:AddTalk("既然到这里来了，就在罪恶值消除之前好好反省反省吧。")
this:AddSendOutPrisonCmd("出狱(0)")
TaskDialog()
this:AddOpenBribeInputBox("贿赂(0)")
this:AddeHonorToPk("荣誉点洗罪恶值(0)")
this:AddDialogItem("出狱方法说明(3)", "Dialog1")
this:AddCloseItem("取消")
end
this:PlaySound("23011.wav,23015.wav,23022.wav,20001.wav,20003.wav,20005.wav,61007.wav,61009.wav,61030.wav")  
function Dialog1()
this:AddTalk("出狱方法说明:时间积累（同一般洗罪恶值方法）\n 对狱卒进行“贿赂”1锭洗50点罪恶值\n 荣誉值洗罪恶值\n 接收劳务任务，在劳务区挖矿，50个矿石可获取1小时的“洗心革面”状态，在该状态下每分钟pk值减少速度为正常的3倍（杀人的话即解除此状态）\n 袭击典狱官并将之杀死")
this:AddCloseItem("离开")
end                                                                                           
TaskDialog()
end
this:AddDialog("MainDialog")</menu>
	</npc>
