this:AddGiveUnionMoney("捐献帮会资金(0)")
this:AddQueryUnionMoney("查询帮会资金(0)")
this:AddDialogItem("关于帮会行动力(3)", "Dialog2")
this:AddDialogItem("关于帮会招家族上限(3)", "Dialog3")
if this:IsAttackPop(0)==true then                                               
this:AddLaunchAttack("发起帮会战斗(0)",0)  
end 
this:PlaySound("20001.wav,20003.wav,20005.wav,61006.wav,61012.wav,61030.wav,61031.wav")                                                                                         
TaskDialog()
end
function Dialog2()
this:AddTalk("行动力是整个帮会展开活动的根本，每天系统会根据家族威望来给每个帮会一定的行动力值，每100点威望会有1点行动力。\n帮会的威望是由帮会中家族声望相加产生的。\n想要获得帮会行动力，提升帮会威望是唯一途径。")
end
function Dialog3()
this:AddTalk("帮会能招收家族最大上限与帮会威望相关，威望越高能招收的家族越多。\n30000威望  上限40个家族\n80000威望  上限50个家族")
end
this:AddDialog("MainDialog") </menu>
	</npc>
