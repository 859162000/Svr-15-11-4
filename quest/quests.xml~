function SaleCartoonPet()
num = this:GetCartoonPetNum()
if( num &gt; 0 ) then
for i=1,num do  
this:AddSaleCartoonPetCmd(i,"ConfirmSaleCartoon")
end
end
this:AddDialogItem("返回","MainDialog")
end

function BuyHorseDialog()
this:ResetHorseOwnStatus()
if(this:GetLevel() &lt; 30) then 
this:AddTalk("等级不够30级不可以买马")
elseif(this:IsHasHorse() == false) then
this:AddTalk("你可以买马，不过要20两哦。")
this:AddBuyItemCmd("买马",3000,1)
else
this:AddTalk("你已经有马了")
end
this:AddDialogItem("返回","MainDialog")
end

function SaleHorseDialog()
if(this:IsHasHorse() == true) then
this:AddTalk("你真的要卖马吗?价格是10两") 
this:AddScriptButton("确定","SellHorse")
else
this:AddTalk("你没有马可以卖")
end
this:AddDialogItem("返回","MainDialog")
end</menu>
